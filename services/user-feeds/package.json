  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "node -r ts-node/register --test $(find ./src -name '*.spec.ts')",
    "test:e2e": "node -r ts-node/register --test 'test/app.e2e-spec.ts'",
    "sync-db": "npx mikro-orm schema:update --run",
    "migration:create": "npx mikro-orm migration:create",
    "migration:list": "npx mikro-orm migration:list",
    "migration:pending": "npx mikro-orm migration:pending",
    "migration:down": "npx mikro-orm migration:down",
    "migration:up": "npx mikro-orm migration:up",
    "migration:blank": "npx mikro-orm migration:create --blank"
  },
